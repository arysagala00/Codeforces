#include<bits/stdc++.h>
 
using namespace std;
 
int main(){
	map<string, int> mp;
	int n;
	string s;
	mp["Tetrahedron"] = 4;
	mp["Cube"] = 6;
	mp["Octahedron"] = 8;
	mp["Dodecahedron"] = 12;
	mp["Icosahedron"] = 20;
	
	cin>>n;
	
	int tot = 0;
	
	while(n--){
		cin>>s;
		tot+=mp[s];
	}
	
	cout<<tot<<endl;
}
